  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "esbuild": "^0.25.0",
    "eslint": "^9.22.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.2"
  }
}
